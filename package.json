{
  "name": "diwala-bacon-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sanity/client": "^0.123.0",
    "@types/react-burger-menu": "^2.2.0",
    "baconjs": "^1.0.1",
    "contentful": "^5.0.5",
    "node-sass-chokidar": "^0.0.3",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-burger-menu": "^2.2.3",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/compiled_css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/compiled_css/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "react-scripts-ts": "^2.13.0",
    "react-social-icons": "^2.8.0",
    "typescript": "^2.7.2"
  }
}
